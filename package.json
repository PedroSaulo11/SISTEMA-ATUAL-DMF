{
  "name": "dmf-contaazul-integration",
  "version": "1.0.0",
  "description": "Backend para integração API Conta Azul no sistema DMF",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:webhook": "node webhook-server.js",
    "dev:webhook": "nodemon webhook-server.js",
    "check:syntax": "node --check server.js && node --check script.js && node --check bootstrap.js",
    "check:security": "node scripts/security-check.js",
    "smoke:health": "node scripts/smoke-health.js",
    "check:phase3": "npm run check:syntax && npm run check:security && npm run smoke:health"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "ngrok": "^5.0.0-beta.2",
    "pg": "^8.11.0",
    "redis": "^4.6.13",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.0.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "overrides": {
    "tar": "^7.5.6"
  },
  "keywords": [
    "dmf",
    "contaazul",
    "api",
    "oauth2"
  ],
  "author": "DMF Enterprise",
